<!DOCTYPE html>

<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Community</title>

    <link rel="stylesheet" asp-href-include="~/css/StyleSheet.css" />
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css">
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.1.1/jquery.min.js"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js"></script>

    <script src="https://ajax.aspnetcdn.com/ajax/jQuery/jquery-1.11.3.min.js"></script> @*add this*@
    <script src="https://ajax.aspnetcdn.com/ajax/jquery.validate/1.14.0/jquery.validate.min.js"></script> @*add this*@
    <script src="https://ajax.aspnetcdn.com/ajax/jquery.validation.unobtrusive/3.2.6/jquery.validate.unobtrusive.min.js"></script>@*add this*@


    @RenderSection("AddToHeadAnotherWay", required: false)
</head>
<body>

    @* <div class="navbar-collapse collapse">*@
    <div class="container-fluid">
        <div class="row content">
            <div class="col-sm-3 sidenav">
                <h4>Always Hungry</h4>
                <ul class="nav nav-pills nav-stacked">
                    <li id="row1"> <a id="a1" asp-area="" asp-controller="Account" asp-action="Register">Register</a></li>
                    <li id="row1"> <a id="a1" asp-area="" asp-controller="Account" asp-action="Login">Login</a></li>
                    <li id="row1"> <a id="a1" asp-area="" asp-controller="Home" asp-action="Index">Home</a></li>
                    <li id="row1"><a id="a1" asp-area="" asp-controller="Home" asp-action="About">About</a></li>
                    <li id="row1"><a id="a1" asp-area="" asp-controller="Home" asp-action="Contact">Contact</a></li>
                    <li id="row1"><a id="a1" asp-area="" asp-controller="Home" asp-action="History">History</a></li>
                    <li id="row1"><a id="a1" asp-area="" asp-controller="Map" asp-action="Map">Map</a></li>
                    <li id="row1"><a id="a1" asp-area="" asp-controller="Member" asp-action="Members">All Members</a></li>
                    <li id="row1"> <a id="a1" asp-area="" asp-controller="Message" asp-action="Messages">All Messages</a></li>
                    <li id="row1"> <a id="a1" asp-area="" asp-controller="Account" asp-action="Logout">Logout</a></li>

                </ul>

            </div>
            <div>
                @await Component.InvokeAsync("CartSummary")

            </div>
            <div class="col-sm-9">
                @RenderBody()
            </div>

        </div>
    </div>
    <div>
        <footer class="container-fluid">

            <p>
                This is the end go back to the top
            </p>

        </footer>
    </div>
</body>
</html>
