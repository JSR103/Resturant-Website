@model List<Restaurant.Models.Message>
@{
    Layout = "_Layout";
}

<!DOCTYPE html>

<html>
<head>
    <title>MessageByMember</title>
</head>
<body>
    <h1>Messages by Member</h1>
    @foreach (var a in Model)
    {
        <br />
        <h3>Subject: @a.Subject </h3>
        <p>Body: @a.Body </p>
        <h2>From: @a.From</h2>

        <hr id="hr1">
    }
</body>

</html>